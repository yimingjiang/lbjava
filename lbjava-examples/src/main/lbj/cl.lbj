package edu.illinois.cs.cogcomp.lbjava.examples.cl;

real[] CLFeatures(AlgoData d) <- {
    for (int i = 0; i < d.getFeatures().length; i++) {
        sense d.getFeatures()[i];
    }
}

discrete CLLabel(AlgoData d) <- {
    return d.getLabel();
}

discrete CLClassifier(AlgoData d) <-
    learn CLLabel
    using CLFeatures

    with SparseNetworkLearner {}
end